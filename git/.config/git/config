[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
[commit]
	gpgsign = true
[tag]
	gpgsign = true
[gpg]
	program = gpg2
[user]
	useConfigOnly = true
[include]
    path = ~/.config/git/work.conf
[fetch]
	prune = true
[push]
	default = current
